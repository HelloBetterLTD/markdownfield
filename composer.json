{
    "name": "silverstripers/markdown",
    "description": "Markdown DB field and form field for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "markdown", "module"],
    "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Nivanka Fonseka",
        "email": "nivanka@silverstripers.com"
    }],
    "require": {
        "silverstripe/framework": "4.1.x-dev",
        "cebe/markdown": "1.1.2"
    },
    "autoload": {
        "psr-4": {
            "SilverStripers\\markdown\\db\\": "src/db",
            "SilverStripers\\markdown\\extensions\\": "src/extensions",
            "SilverStripers\\markdown\\forms\\": "src/forms",
            "SilverStripers\\markdown\\shortcodes\\": "src/shortcodes"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ],
        "installer-name": "markdown"
    }
}
